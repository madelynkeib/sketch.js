
let xmove = 200
function setup() {
  createCanvas(400, 400);
}

function draw() {
  background('white');
  for (let i; i < 5; i++){
  person(xmove)
  xmove += 5
   
  }
  

}
function person(x){
      ellipse(x,150,40)
      stroke(10)
      line(x,170,200,250)
      line(x,250,140,330)
      line(x,250,250,330)
      line(x,210,150,200)
      line(x,210,250,200)
      
  }
// im trying to make the person move in a loop by chainging the X values 
