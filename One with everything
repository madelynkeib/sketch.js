function setup() {
  createCanvas(710, 400, WEBGL);
}

function draw() {
  background('red');
  noStroke();
  fill(60);
  push();
  translate(-200, 40);
  rotateY(1.25);
  rotateX(-0.9);
  box(100);
  pop();

  noFill();
  stroke(255);
  push();
  translate(500, height * 0.35, -200);
  sphere(300);
  pop();
}
//I based this on the exmple from the reference and made some changes
